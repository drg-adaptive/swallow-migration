{
  "name": "swallow-cli",
  "description": "A migration utility for AWS resources",
  "version": "1.0.2",
  "author": "Ben Force",
  "bin": {
    "swallow": "./bin/run"
  },
  "bugs": "https://github.com/drg-adaptive/swallow-migration/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "js-yaml": "^3.13.1",
    "swallow-plugins": "^1.0.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^10",
    "globby": "^10",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/drg-adaptive/swallow-migration",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "swallow",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "drg-adaptive/swallow-migration",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "build": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme"
  },
  "types": "lib/index.d.ts"
}
